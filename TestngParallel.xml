<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Parallel Execution" verbose="1" parallel="tests" thread-count="5">

	<test name="Login in First Mobile">
		<parameter name="port" value="4723" />
		<parameter name="mbrowser" value="firefox" />
		<parameter name="platformName" value="Android" />
		<parameter name="sleep" value="1000" />
		<parameter name="Parallel" value="Yes" />
		<classes>
			<class name="com.appium.eBay.scripts.login" />
		</classes>
	</test>

	<test name="Login in second mobile">
		<parameter name="port" value="4738" />
		<parameter name="mbrowser" value="firefox" />
		<parameter name="platformName" value="Android" />
		<parameter name="sleep" value="1000" />
		<parameter name="Parallel" value="Yes" />
		<classes>
			<class name="com.appium.eBay.scripts.loginfail" />
		</classes>
	</test>
</suite> 
